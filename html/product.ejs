<html>
    <head>
        <%- include('head'); %>   
    </head>
    <body>
        <!-- Navigation bar -->
        <nav class="navbar navbar-expand-lg navbar-light fixed-top bg-gr-transparent">
            <%- include('navbar'); %>          
        </nav>
        <section class="mt-6">
            <div class="container">
                <h2 class="text-left text-dr-br slide">Products</h2>
                <div class="row">
                    <% products.forEach(function(product){ %>
                        <%- include('product_template', product); %>
                    <% }); %>
                </div>
            </div>
        </section>
        <!-- Footer -->
        <footer class="pt-4 mt-4  bg-lg-bl">
            <%- include('footer'); %>
        </footer>
    
        <!-- Bootstrap JavaScript -->
        <script src="js/bootstrap.js"></script>
        <script src="js/bootstrap.bundle.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

        <!-- Local script-->
        <script src="js/slide.js"></script>

        <!-- Font awesom script-->
        <script src="https://kit.fontawesome.com/7ce695a48c.js" crossorigin="anonymous"></script>
    </body>
</html>